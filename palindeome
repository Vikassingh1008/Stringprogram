import java.util.*;
public class Main
{
	public static void main(String[] args) {
		String str="bcbb";
		String gtr="";
		for(int i=str.length()-1;i>=0;i--)
		{
		    gtr+=str.charAt(i);
		}
	
 			     
 			    if(str.equals(gtr))
    		{
    		    System.out.println("plandrome");  
    		    }    
    		    else{
     		     System.out.println("not plandrome");
    		}
			
	
	}
}
